{
  "title": "HuaiBook's Title",
  "author": "Huai",
  "description": "记笔记的地方",
  "language": "zh-hans",
  "gitbook": "3.2.3",
  "styles": {
    "website": "./styles/website.css"
  },
  "structure": {
    "readme": "README.md"
  },
  "plugins": [
    "-sharing",
    "expandable-chapters",
    "anchors",
    "anchor-navigation-ex",
    "splitter"
  ],
  "pluginsConfig": {
    "sharing": {
      "douban": false,
      "facebook": false,
      "google": false,
      "hatenaBookmark": false,
      "instapaper": false,
      "line": false,
      "linkedin": false,
      "messenger": false,
      "pocket": false,
      "qq": true,
      "qzone": false,
      "stumbleupon": false,
      "twitter": false,
      "viber": false,
      "vk": false,
      "weibo": true,
      "whatsapp": false,
      "all": [
        "google", "facebook", "weibo", "twitter",
        "qq", "qzone", "linkedin", "pocket"
      ]
    }
  }
}
